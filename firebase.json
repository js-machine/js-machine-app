{
  "hosting": [
    {
      "target": "js-machine-admin",
      "public": "js_machine_admin/build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/getDigests",
          "function": "getDigests"
        },
        {
          "source": "/createDigest",
          "function": "createDigest"
        },
        {
          "source": "/saveDigest",
          "function": "saveDigest"
        },
        {
          "source": "/deleteDigest",
          "function": "deleteDigest"
        },
        {
          "source": "/hideDigest",
          "function": "hideDigest"
        },
        {
          "source": "/showDigest",
          "function": "showDigest"
        },
        {
          "source": "/uploadDigestMd",
          "function": "uploadDigestMd"
        },
        {
          "source": "/getFeedbacks",
          "function": "getFeedbacks"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "js-machine-app",
      "public": "js_machine_front/build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/getAllEvents",
          "function": "getAllEvents"
        },
        {
          "source": "/getRecentEvents",
          "function": "getRecentEvents"
        },
        {
          "source": "/getDigests",
          "function": "getDigests"
        },
        {
          "source": "/sendFeedback",
          "function": "sendFeedback"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "functions": {},
  "firestore": {
    "rules": "firestore.rules"
  },
  "emulators": {
    "firestore": {
      "port": "5002"
    }
  }
}
